			{
				"label": "Prism (No Bold)",
				"uiTheme": "vs",
				"path": "./themes/Prism-color-theme-no-bold-minimized.json"
			}
		]
	},
	"devDependencies": {
		"@types/node": "^12.20.6",
		"json5": "^2.2.0"
	}
