{
	"name": "prism",
	"displayName": "Prism",
	"description": "Prism.js default light theme.",
	"version": "1.0.3",
	"publisher": "usernamehw",
	"repository": {
		"type": "git",
		"url": "https://github.com/usernamehw/vscode-theme-prism"
	},
	"icon": "img/icon.png",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/usernamehw/vscode-theme-prism/issues"
	},
	"engines": {
		"vscode": "^1.20.0"
	},
	"qna": false,
	"keywords": [
		"theme",
		"themes",
		"Light",
		"Light Theme"
	],
	"categories": [
		"Themes"
	],
	"scripts": {
		"vscode:prepublish": "node minimize.js"
	},
	"contributes": {
		"themes": [
			{
				"label": "Prism",
				"uiTheme": "vs",
