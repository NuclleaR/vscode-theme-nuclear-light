{
	"name": "nuclear-light",
	"displayName": "NucleaR'z Theme Light",
	"description": "Light theme for ... eyes or something.",
	"version": "1.0.0",
	"publisher": "NuclleaR",
	"repository": {
		"type": "git",
		"url": "https://github.com/NuclleaR/vscode-theme-nuclear-light"
	},
	"icon": "img/icon.png",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/NuclleaR/vscode-theme-nuclear-light/issues"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"qna": false,
	"keywords": [
		"theme",
		"light",
		"soft",
		"mild",
		"warm",
		"easy",
		"eyes",
		"light theme"
	],
	"categories": [
		"Themes"
	],
	"scripts": {
		"vscode:prepublish": "node minimize.js",
		"watch": "nodemon --watch themes/Nuclear-color-theme-light.json --exec \"npm run vscode:prepublish\""
	},
	"contributes": {
		"themes": [
			{
				"label": "Nuclear'z Light",
				"uiTheme": "vs",
				"path": "./themes/Nuclear-color-theme-light-minimized.json"
			},
			{
				"label": "Nuclear'z Light (No Bold)",
				"uiTheme": "vs",
				"path": "./themes/Nuclear-color-theme-light-no-bold-minimized.json"
			}
		]
	},
	"devDependencies": {
		"@types/node": "^18.14.0",
		"json5": "^2.2.3",
		"nodemon": "^2.0.20"
	}
}